    "version": "0.2.0",
    "configurations": [
        {
            "args": [
                "task"
            ],
            "name": "Gulp task",
            "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },

        {
            "name": "Fit Generated",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/examples/fit-generated-data.py",
            "cwd": "${workspaceFolder}/examples",
            "console": "integratedTerminal"
        }
    ]
}